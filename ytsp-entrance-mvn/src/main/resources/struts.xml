<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

    <package name="default" namespace="/data" extends="struts-default">
        <action name="sessionList" class="com.ytsp.entrance.action.SessionUserAction" method="execute">
            <result name="success">/data/session_user.jsp</result>
            <result name="login">/data/denglu.jsp</result>
        </action>
        <action name="productDetail" class="com.ytsp.entrance.action.ProductDetailAction" method="execute">
            <result name="phone">/data/productDetailPhone.jsp</result>
            <result name="pad">/data/productDetailPad.jsp</result>
        </action>
        <action name="productParam" class="com.ytsp.entrance.action.ProductParamAction" method="execute">
            <result name="phone">/data/productParamPhone.jsp</result>
            <result name="pad">/data/productParamPad.jsp</result>
        </action>
        <action name="productComments" class="com.ytsp.entrance.action.ProductCommentsAction" method="execute">
            <result name="phone">/data/productCommentPad.jsp</result>
            <result name="pad">/data/productCommentPhone.jsp</result>
        </action>
        <action name="createUserAction" class="com.ytsp.entrance.action.CreateUserAction" method="execute">
            <result name="SUCCESS">/data/success</result>
        </action>
        <action name="yimaGoodsAction" class="com.ytsp.entrance.action.YimaGoodsAction" method="execute">
            
        </action>
    </package>

</struts>